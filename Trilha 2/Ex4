class Temperatura {
    private valor: number;

    constructor(valor: number) {
        this.valor = valor;
    }

    public paraFahrenheit(): number {
        return (this.valor * 9/5) + 32;
    }

    public paraKelvin(): number {
        return this.valor + 273.15;
    }

    public obterValorCelsius(): number {
        return this.valor;
    }
}